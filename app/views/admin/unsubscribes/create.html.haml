
.row
  .span2
    = render partial: 'navigation'
  .span10
    %h2 Unsubscribes

    %h4
      Upload Report

    %table.table.table-striped.table-condensed.sortable
      %thead
        %tr
          %th Stat
          %th Count
      %tbody
        %tr
          %td
            Lines Uploaded
          %td
            = @lines

        %tr
          %td
            Members Found
          %td
            = @members_found

        %tr
          %td
            Unusbscribes Created
          %td
            = @unsubscribes_created
