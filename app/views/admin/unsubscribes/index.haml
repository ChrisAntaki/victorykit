%div.row
  %div.span12
    %div.page-header
      %h1 Unsubscribes

      = render partial: "admin/members/navigation", locals: { active: :unsubscribes }
      = render partial: "admin/unsubscribes/subnav", locals: { active: nil }

      %div.clearfix

%div.row
  %div.span12
    = render partial: "shared/pagination", locals: { collection: @unsubscribes }

    %table.table.table-striped.table-condensed.sortable
      %thead
        %tr
          %th Name
          %th Created
      %tbody
        - @unsubscribes.each do |unsubscribe|
          %tr
            %td #{unsubscribe.email}
            %td #{unsubscribe.created_at}

    = render partial: "shared/pagination", locals: { collection: @unsubscribes }
