
.row
  .span2
    = render partial: 'navigation'
  .span10
    %h2 Unsubscribes
    = form_tag(admin_unsubscribes_path(format: 'csv'), :method => 'get') do
      %p
        %label{:for => 'from'} From:
        %input#from{:type => 'date', :name=>'from'}
      %p
        %label{:for => 'from'} To:
        %input#to{:type => 'date', :name=>'to'}
      %p
        = submit_tag 'Download CSV'



